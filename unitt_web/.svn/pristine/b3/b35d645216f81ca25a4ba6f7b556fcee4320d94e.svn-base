package bean;

public class BoardBean {
	private int courseboard_idx;
	private String member_id;
	private String category;
	private String title;	
	private String content;
	private String image;
	private String shot_introduce;	
	private String plan;
	private String wdate; 
	private int board_count;
	
	public BoardBean(String id,String category,String title, 
			String content, String image, String shot_introduce, String plan, String wdate){
		
		this.member_id = id;
		this.category = category;
		this.title = title;
		this.content = content;
		this.image = image;
		this.shot_introduce = shot_introduce;
		this.plan = plan;
		this.wdate = wdate;
	}
	
	public int getCourseboard_idx() {
		return courseboard_idx;
	}
	public void setCourseboard_idx(int courseboard_idx) {
		this.courseboard_idx = courseboard_idx;
	}
	public String getMember_id() {
		return member_id;
	}
	public void setMember_id(String member_id) {
		this.member_id = member_id;
	}
	public String getCategory() {
		return category;
	}
	public void setCategory(String category) {
		this.category = category;
	}
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	public String getImage() {
		return image;
	}
	public void setImage(String image) {
		this.image = image;
	}
	public String getShot_introduce() {
		return shot_introduce;
	}
	public void setShot_introduce(String shot_introduce) {
		this.shot_introduce = shot_introduce;
	}
	public String getPlan() {
		return plan;
	}
	public void setPlan(String plan) {
		this.plan = plan;
	}
	public String getWdate() {
		return wdate;
	}
	public void setWdate(String wdate) {
		this.wdate = wdate;
	}
	public int getBoard_count() {
		return board_count;
	}
	public void setBoard_count(int board_count) {
		this.board_count = board_count;
	}
}
